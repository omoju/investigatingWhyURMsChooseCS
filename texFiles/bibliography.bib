	Issn = {0278-7393},
	Journal = {Journal of experimental psychology. Learning, memory, and cognition},
	Keywords = {Association Learning,Concept Formation,Discrimination Learning,Humans,Judgment,Linguistics,Models,Probability Learning,Problem Solving,Psychological,Semantics},
